<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <title>Gap Data Error</title>
    <link rel="stylesheet" type="text/css" href="/top.css" />
    <link rel="icon" href="/icons/favicon.ico" />
  </head>
  <body>
    <div class="logon">{{>logon}}
    </div>
    <div class="block">
      {{#error}}
      <div class="block inside">
        <h1 class="errors"> {{=error_status}}</h1>
        <p class="errors"> {{=error_message}}
        </p>
        <pre class="errors">
{{=error_exception}}
        </pre>
      </div>
      {{/error}}
    </div>
    <div class="logo rhs" onclick="javascript:document.location = 'http://gap-data.appspot.com/'; return true;" />
  </body>
</html>
